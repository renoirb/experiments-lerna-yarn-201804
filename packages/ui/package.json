{
  "name": "ui",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/ui.cjs.js",
  "module": "dist/ui.esm.js",
  "author": "Renoir Boulanger <renoir.boulanger@cgi.com>",
  "scripts": {
    "build:dist": "node -r esm scripts/build.js",
    "build:lib": "node -r esm scripts/build-lib.js",
    "build": "npm run build:lib && npm run build:dist",
    "postbuild": "node -r esm scripts/copyright.js",
    "test": "vue-cli-service test",
    "test:watch": "vue-cli-service test --config jest.config.js -i --watch",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "dev": "vue-cli-service serve --open --hot"
  },
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "replace-in-file": "^3.4.0",
    "@miljan/build": "^0.4.2",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "css-loader": "^0.28.11",
    "eslint-config-experiment": "*",
    "eslint-loader": "^2.0.0",
    "esm": "^3.0.16",
    "jest-css-modules": "^1.1.0",
    "jest-vue-preprocessor": "^1.4.0",
    "rollup": "^0.57.0",
    "rollup-analyzer": "^1.1.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.16",
    "vuetify": "^1.0.11"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist"
  ],
  "@std/esm": {
    "mode": "js",
    "cjs": true
  },
  "eslintConfig": {
    "globals": {
      "ENV": true
    },
    "env": {
      "jest": true
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/../../node_modules",
      "<rootDir>/../../packages"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
